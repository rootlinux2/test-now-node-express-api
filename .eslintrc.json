{
    "extends": ["airbnb", "prettier", "plugin:jasmine/recommended"],
    "plugins": ["prettier", "jasmine"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jasmine": true
    },
    "rules": {
        "prettier/prettier": ["error"]
    }
}